<script type="text/javascript">
	jQuery(document).ready(function(){
		jQuery("ficha_fic_data").datepicker()
	});
</script>
<h1>New ficha</h1>
<% form_for(@ficha) do |f| %>
<%= f.error_messages %>
<p>
    <%= f.label :dentista_id %>
    <br/>
    <%= f.text_field :dentista_id %>
</p>
<p>
    <%= f.label :paciente_id %>
    <br/>
    <%= f.text_field :paciente_id %>
</p>
<p>
    <%= f.label :fic_data %>
    <br/>
    <!-- <%= f.date_select :fic_data %> -->
    <%= datepicker_tag :ficha, :fic_data, :size => 10 %>
</p>
<p>
    <%= f.label :fic_desc %>
    <br/>
    <%= f.text_area :fic_desc %>
</p>
<p>
    <%= f.submit 'Create' %>
</p>
<% end %>
<%= link_to 'Back', fichas_path %>